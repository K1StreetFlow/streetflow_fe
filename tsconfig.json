{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "services/**/*.js",
    "app/dashboard/page.js",
    "app/page.js",
    "app/dashboard/order-list/page.js",
    "components/Tables/TableOrder.js",
    "app/auth/admin/login/page.jsx",
    "app/auth/admin/register/page.jsx",
    "components/Header/DropdownUser.jsx",
    "app/dashboard/profile/page.jsx",
    "components/Dashboard/E-commerce.jsx",
    "components/Dashboard/Cart/TableTwo.jsx",
    "components/Dashboard/Cart/TableCart.jsx"
  ],
  "exclude": ["node_modules"]
}
